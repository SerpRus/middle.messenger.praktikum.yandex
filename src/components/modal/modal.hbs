<div id="{{modalId}}"
     class="{{classes}}
     modal
     {{#if thema}}modal--{{thema}}{{/if}}"
     {{#each attributes}}
        {{this.key}}="{{this.value}}"
     {{/each}}
>
    <div class="modal__overlay"></div>

    <div class="modal__content">
        {{> @partial-block}}
    </div>
</div>
