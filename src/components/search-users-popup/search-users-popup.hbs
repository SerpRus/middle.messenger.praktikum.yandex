{{#Modal classes=classes id=id }}
    <div class="modal__content">
        <h2 class="modal__title">
            {{ title }}
        </h2>

        <div class="modal__content-inner">
            {{{SearchUsers
                fieldId=fieldId
                fieldName=fieldName
                fieldType=fieldType
                fieldLabel=fieldLabel
                fieldValue=fieldValue
                fieldRef=fieldRef
                fieldOnInput=fieldOnInput
            }}}
        </div>
    </div>
{{/Modal}}
