{{#Modal classes=classes id=id }}
    <div class="modal__content">
        <h2 class="modal__title">
            {{ title }}
        </h2>

        <div class="modal__content-inner">
            {{{UsersList
                classes="search-users__users"
                users=users
                iconName="delete"
                iconTheme="highlighted"
                eventBusName=eventBusName
            }}}
        </div>
    </div>
{{/Modal}}
