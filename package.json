{
  "name": "chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "18.8.0"
  },
  "scripts": {
    "dev": "npm run slfix && vite",
    "sl": "npx stylelint \"**/*.scss\"",
    "slfix": "npx stylelint \"**/*.scss\" --fix",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lintfix": "eslint \"**/*.{ts,tsx}\" --fix",
    "build": "tsc && vite build",
    "start": "npm run build && node src/server.js",
    "test": "mocha",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/chai": "^4.3.6",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.3",
    "@types/mocha": "^10.0.2",
    "@types/proxyquire": "^1.3.29",
    "@types/sinon": "^10.0.17",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "chai": "^4.3.10",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "esm-loader-css": "^1.0.5",
    "esm-loader-typescript": "^1.0.5",
    "esmock": "^2.5.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "mocha": "^10.2.0",
    "node-esm-loader": "^0.2.5",
    "optionator": "^0.9.3",
    "proxyquire": "^2.1.3",
    "sass": "^1.64.1",
    "sinon": "^16.0.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "handlebars": "^4.7.7",
    "nanoid": "^4.0.2",
    "vite-plugin-clean": "^1.0.0",
    "vite-plugin-handlebars": "^1.6.0"
  }
}
